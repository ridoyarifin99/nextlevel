{
  "rewrites": [
    {
      "source": "/(best-selling|streaming|music|storage|vpn|aiDesign|combos|education|adult)",
      "destination": "/index.html"
    }
  ]
  
  "routes": [
    {
      "src": "/",
      "dest": "/index.html",
      "headers": { "x-vercel-ip-subdomain": "nextlevelbd.store" }
    },
    {
      "src": "/",
      "dest": "/details.html",
      "headers": { "x-vercel-ip-subdomain": "details.nextlevelbd.store" }
    },
    {
      "src": "/",
      "dest": "/payments.html",
      "headers": { "x-vercel-ip-subdomain": "payments.nextlevelbd.store" }
    }
  ]

}
